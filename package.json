{
  "name": "room-reservation-api",
  "version": "1.0.0",
  "description": "Meeting room reservation API built with Fastify and TypeScript",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "node --import tsx --test tests/**/*.test.ts",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write ."
  },
  "keywords": [
    "fastify",
    "typescript",
    "api",
    "reservations"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "fastify": "^5.2.1",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.10.10",
    "@types/uuid": "^10.0.0",
    "pino-pretty": "^13.1.3",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}